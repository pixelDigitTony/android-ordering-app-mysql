<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ordersystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".changeProfile"></activity>
        <activity android:name=".profile" />
        <activity android:name=".menuEditCategory" />
        <activity android:name=".menuReport" />
        <activity android:name=".menuRemDish" />
        <activity android:name=".menuRemCategory" />
        <activity android:name=".menuAddDish" />
        <activity
            android:name=".menuAddCategory"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".menuSetting" />
        <activity android:name=".orderBill" />
        <activity android:name=".orderStatus" />
        <activity android:name=".Cart" />
        <activity android:name=".dishDetail" />
        <activity
            android:name=".orderDish"
            android:parentActivityName=".orderMenu" />
        <activity
            android:name=".orderMenu"
            android:label="@string/title_activity_order_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".register" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>